{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.24.0",
    "formik": "^2.4.2",
    "github-slugger": "^2.0.0",
    "lodash": "^4.17.21",
    "mdast-util-to-hast": "^12.3.0",
    "mdast-util-toc": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.2",
    "react-markdown": "^8.0.7",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.14.0",
    "remark-breaks": "^3.0.3",
    "remark-gfm": "^3.0.1",
    "typescript": "^4",
    "zustand": "^4.3.8"
  },
  "scripts": {
    "start": "vite serve --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "tsc": "tsc --strict",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": [
        "warn",
        {
          "additionalHooks": "usePageMetadata2"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/github-slugger": "^2.0.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.2",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^6.0.1"
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ]
  }
}
